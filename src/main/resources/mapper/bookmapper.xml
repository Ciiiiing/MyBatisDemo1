<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.BookDao">
    <select id="findAll" resultType="com.example.demo.Book">
        select *
        from book
    </select>
    <insert id="add" useGeneratedKeys="true" keyProperty="id" parameterType="com.example.demo.Book">
        insert into book (name)
        values ('c')
    </insert>
</mapper>